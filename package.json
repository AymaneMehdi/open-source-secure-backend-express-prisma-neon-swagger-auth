{
  "name": "open-source-secure-backend-express-prisma-neon-swagger-auth",
  "version": "1.0.0",
  "description": "Open source secure backend template using Express.js for REST APIs, Prisma ORM with Neon for PostgreSQL, Redis for session management, Passport for authentication (local, Google, GitHub), JWT for authorization, Swagger for API documentation, and security best practices (Helmet, rate limiting, CORS, bcryptjs).",
  "main": "index.js",
  "keywords": [
    "express",
    "nodejs",
    "postgresql",
    "prisma",
    "rest-api",
    "neon",
    "swagger",
    "swagger-jsdoc",
    "swagger-ui-express",
    "passport",
    "passport-local",
    "passport-github2",
    "passport-google-oauth20",
    "auth",
    "authentication",
    "authorization",
    "jwt",
    "jsonwebtoken",
    "bcryptjs",
    "redis",
    "connect-redis",
    "express-session",
    "cookie-parser",
    "cors",
    "dotenv",
    "helmet",
    "morgan",
    "express-rate-limit",
    "express-validator",
    "nodemon",
    "backend",
    "api",
    "secure",
    "open-source"
  ],
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "prisma": "prisma",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "db:reset": "prisma db push --force-reset && npm run db:seed",
    "test": "node test-api.js"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "bcryptjs": "^3.0.2",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "redis": "^5.8.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "prisma": "^5.22.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "author": "Aymane Mehdi",
  "license": "ISC"
}
